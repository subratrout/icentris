<p>
  No account? <%= link_to 'Sign up!', signup_path %>
</p>
<header id="content-header">
  <h1>Sign In</h1>
</header>
<div class="row">
  <%= form_tag(session_path) do %>
    <fieldset>
      <ol>
        <li class="required">
          <%= label_tag :email %>
          <%= email_field_tag :email, nil, required:true, autofocus: true, size: 40 %>
        </li>
        <li class="required">
          <%= label_tag :password %>
          <%= password_field_tag :password, nil,required:true, size: 40 %>
        </li>
      </ol>
      <p>
        <%= submit_tag 'Sign In', class: "btn btn-primary" %>
      </p>
    </fieldset>
  <% end %>
</div>
